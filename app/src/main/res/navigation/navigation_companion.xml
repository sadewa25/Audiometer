<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/navigation_companion"
    app:startDestination="@id/dashboardCompanionFragment">

    <fragment
        android:id="@+id/dashboardCompanionFragment"
        android:name="com.codedirect.pulmos.ui.dashboard.companion.DashboardCompanionFragment"
        android:label="DashboardCompanionFragment" >
        <action
            android:id="@+id/action_dashboardCompanionFragment_to_reportNeededFragment2"
            app:destination="@id/reportNeededFragment2" />
        <action
            android:id="@+id/action_dashboardCompanionFragment_to_reportSymptomsFragment2"
            app:destination="@id/reportSymptomsFragment2" />
        <action
            android:id="@+id/action_dashboardCompanionFragment_to_patientCompanionFragment"
            app:destination="@id/patientCompanionFragment" />
        <action
            android:id="@+id/action_dashboardCompanionFragment_to_profileCompanionFragment"
            app:destination="@id/profileCompanionFragment" />
    </fragment>
    <fragment
        android:id="@+id/reportNeededFragment2"
        android:name="com.codedirect.pulmos.ui.report_needed.ReportNeededFragment"
        android:label="ReportNeededFragment" />
    <fragment
        android:id="@+id/reportSymptomsFragment2"
        android:name="com.codedirect.pulmos.ui.report_symptoms.ReportSymptomsFragment"
        android:label="ReportSymptomsFragment" />
    <fragment
        android:id="@+id/patientCompanionFragment"
        android:name="com.codedirect.pulmos.ui.patient_companion.PatientCompanionFragment"
        android:label="PatientCompanionFragment" />
    <fragment
        android:id="@+id/profileCompanionFragment"
        android:name="com.codedirect.pulmos.ui.profile_companion.ProfileCompanionFragment"
        android:label="ProfileCompanionFragment" >
        <action
            android:id="@+id/action_profileCompanionFragment_to_changePasswordFragment2"
            app:destination="@id/changePasswordFragment2" />
    </fragment>
    <dialog
        android:id="@+id/changePasswordFragment2"
        android:name="com.codedirect.pulmos.ui.change_password.ChangePasswordFragment"
        android:label="ChangePasswordFragment" />
</navigation>